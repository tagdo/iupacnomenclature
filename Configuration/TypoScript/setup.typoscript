# IUPAC Nomenclature TypoScript Configuration
# This file is part of the IUPAC Nomenclature TYPO3 extension.
plugin.tx_iupacnomenclature {
    view {
        templateRootPaths {
            10 = EXT:iupacnomenclature/Resources/Private/Templates/
        }
        partialRootPaths {
            10 = EXT:iupacnomenclature/Resources/Private/Partials/
        }
        layoutRootPaths {
            10 = EXT:iupacnomenclature/Resources/Private/Layouts/
        }
    }
}

page.includeCSS.iupacNomenclature = EXT:iupacnomenclature/Resources/Public/Css/iupac.css

page.includeJSFooter {
    iupacNomenclature = EXT:iupacnomenclature/Resources/Public/JavaScript/chemical-compound.js
}